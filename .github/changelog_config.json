{
  "categories": [
    {
      "title": "## ğŸš€ Features",
      "labels": ["feat", "feature"]
    },
    {
      "title": "## ğŸ› Bug Fixes",
      "labels": ["fix", "bug"]
    },
    {
      "title": "## ğŸ§° Maintenance",
      "labels": ["chore", "refactor", "docs", "style", "test"]
    },
    {
      "title": "## ğŸ’¬ Other",
      "labels": []
    }
  ],
  "template": "#{{CHANGELOG}}",
  "pr_template": "- #{{TITLE}}",
  "empty_template": "- No changes found",
  "ignore_labels": ["ignore"],
  "label_extractor": [
    {
      "pattern": ".*bump version to.*",
      "target": "ignore"
    },
    {
      "pattern": "^feat.*:",
      "target": "feat"
    },
    {
      "pattern": "^fix.*:",
      "target": "fix"
    },
    {
      "pattern": "^chore.*:",
      "target": "chore"
    },
    {
      "pattern": "^docs.*:",
      "target": "docs"
    },
    {
      "pattern": "^refactor.*:",
      "target": "refactor"
    },
    {
      "pattern": "^style.*:",
      "target": "style"
    },
    {
      "pattern": "^test.*:",
      "target": "test"
    }
  ]
}
